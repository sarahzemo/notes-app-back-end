{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console" :"off",
        "indent":"off",
        "padded-blocks":"off",
        "spaced-comment":"off",
        "quotes":"off",
        "space-before-blocks":"off",
        "key-spacing":"off",
        "linebreak-style":"off"
    }
}
